{
 "extends": "../../tsconfig.json", // 親の設定をマージ
 "compilerOptions": {
  "baseUrl": ".", // モジュールのベースURL
  "typeRoots": [
   "./src/types",
   "./node_modules/@types"
  ],
  "outDir": "./dist", // 出力先ディレクトリ
  "paths": {
   "@public/*": [
    "../../public/*"
   ],
   "@assets/*": [
    "./assets/*"
   ],
   "@/*": [
    "./src/*"
   ],
  }
 },
 // コンパイル対象ファイル
 "include": [
  "src/**/*.ts",
  "src/**/*.d.ts",
  "src/**/*.vue"
 ],
}