{
 "extends": "../../tsconfig.json", // ルートの設定を拡張
 "compilerOptions": {
  "baseUrl": ".", // モジュールのベースURL
  "outDir": "./dist", // 出力先ディレクトリ
  "paths": {
   "@type/*": [
    "../../public/types/*"
   ],
   "@common/*": [
    "../../templates/common/*"
   ],
  }
 },
 // コンパイル対象ファイル
 "include": [
  "src/**/*.ts",
  "src/**/*.d.ts",
  "src/**/*.vue"
 ],
}